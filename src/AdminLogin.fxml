<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" styleClass="anchor" stylesheets="@adminLogin.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminLoginController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="552.0" layoutY="320.0" prefHeight="116.0" prefWidth="176.0" spacing="5.0">
         <children>
            <JFXTextField id="username" fx:id="username" alignment="CENTER" focusColor="WHITE" promptText="username" styleClass="txtfldusername" stylesheets="@adminLogin.css" text="username" unFocusColor="TRANSPARENT" VBox.vgrow="NEVER">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField id="password" fx:id="password" alignment="CENTER" focusColor="WHITE" promptText="Password" styleClass="txtfldusername" stylesheets="@adminLogin.css" text="password" unFocusColor="TRANSPARENT" VBox.vgrow="NEVER">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <rotationAxis>
                  <Point3D />
               </rotationAxis>
            </JFXPasswordField>
            <JFXButton id="loginbutton" fx:id="btnLogin" alignment="CENTER" contentDisplay="CENTER" onAction="#loginBtnWasClicked" styleClass="txtfldusername" stylesheets="@adminLogin.css" text="Login" textAlignment="CENTER" VBox.vgrow="NEVER">
               <font>
                  <Font name="Bodoni 72 Smallcaps Book" size="16.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" left="30.0" right="30.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </JFXButton>
         </children>
      </VBox>
      <Button id="btnHome" fx:id="btnHome" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#homeBtnWasClicked" styleClass="btnHome" stylesheets="@adminLogin.css" text="Home" AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="100.0">
         <font>
            <Font name="Courier New Bold" size="16.0" />
         </font>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button id="btnCustSupport" fx:id="btnCustSupport" alignment="CENTER" contentDisplay="CENTER" layoutX="90.0" layoutY="110.0" mnemonicParsing="false" onAction="#custSupportBtnWasClicked" styleClass="btnCustSupport" stylesheets="@adminLogin.css" text="Customer Support" textAlignment="CENTER" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="100.0">
         <font>
            <Font name="Courier New Bold" size="16.0" />
         </font>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
