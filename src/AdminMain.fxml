<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminMainController">
   <children>
      <Pane prefHeight="800.0" prefWidth="1280.0">
         <children>
            <WebView fx:id="adminMain" onMouseEntered="#initialize" prefHeight="800.0" prefWidth="1280.0" />
         </children>
      </Pane>
      <JFXButton fx:id="btnSignOut" onAction="#signOutBtnClicked" style="-fx-background-color: rgba(0, 0, 0, 1);" text="Sign Out" textFill="WHITE" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <font>
            <Font name="Courier New Bold" size="16.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
